{
  "name": "google-books",
  "version": "1.0.0",
  "description": "Google Books",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install npm && run build",
    "start:prod": "node server.js",
    "start:dev": "concurrently -k \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },

  "engines": {
    "node": "10.16.0"
},

  "author": "jessica vc",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.6",
    "react-router-dom": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessicavc/google-book-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/jessicavc/google-book-scraper/issues"
  },
  "homepage": "https://github.com/jessicavc/google-book-scraper#readme"
}